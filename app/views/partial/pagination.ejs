<div class="container">
    <ul class="pagination">
        <% if (paginate.hasPreviousPages) {%>
        <li ><a href="<% paginate.href(true) %>">«</a></li>
        <% } else {%>
        <li class="disabled"><a href="#">«</a></li>
        <% } %>

        <% if (pages) {%>
            <% for (var i =0 ;i<pages.length;i++) {%>
                <li><a href="<%= pages[i].url %>"><%= pages[i].number %></a></li>
            <% } %>
        <% } %>

        <% if (paginate.hasNextPages(pageCount)) {%>
        <li ><a href="<% paginate.href(true) %>">«</a></li>
        <% } else {%>
        <li  class="disabled"><a href="#">»</a></li>
        <% } %>
    </ul>
</div>